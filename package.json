{
  "name": "@makerx/typeorm-toolkit",
  "version": "2.0.0",
  "description": "MakerX TypeORM toolkit",
  "author": "MakerX",
  "scripts": {
    "build": "npm run clean && tsc && npm run build:copy-files && npm run build:clean-install && npm run build:post-build",
    "clean": "rimraf build",
    "build:copy-files": "copyfiles \"package*.json\" build",
    "build:clean-install": "cd build && npm clean-install --omit=dev && cd ..",
    "build:post-build": "rimraf build/package-lock.json",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "bin": {
    "typeorm-toolkit": "index.js"
  },
  "dependencies": {
    "pg": "^8.11.3",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/aws-lambda": "^8.10.125",
    "@types/node": "^20.8.7",
    "@types/uuid": "^9.0.6",
    "@types/cross-spawn": "^6.0.2",
    "@types/prompt-sync": "^4.2.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1478.0",
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "colors": "^1.4.0",
    "cross-spawn": "^7.0.3",
    "prompt-sync": "^4.2.0"
  }
}
